#:kivy 1.0.9

<TicTacToeGame>:
    canvas:
        Rectangle:
            pos: 600,175
            size: 10, 300
        Rectangle:
            pos: 700,175
            size: 10, 300
        Rectangle:
            pos: 500,375
            size: 300,10
        Rectangle:
            pos: 500,275
            size: 300,10
    Label:
        id:L
        font_size: 50
        center_x: root.width / 2
        top: root.top - 80
        text:""
    Button:
        id:b0
        font_size:60
        pos:500,50
        size:300,100
        text: "Try again"
        disabled: True
        on_release: root.try_again()
    Button:
        id: b1
        font_size: 60
        pos: 499,385
        size: 101,90
        disabled: True
        on_release: 
            root.change_text(1)
            root.check_win()
    Button:
        id: b2
        font_size: 60
        pos: 499,285
